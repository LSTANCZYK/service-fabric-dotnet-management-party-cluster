@inject Microsoft.ApplicationInsights.AspNetCore.JavaScriptSnippet JavaScriptSnippet
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Microsoft Azure Service Fabric Party Clusters</title>
    <link rel="stylesheet" href="~/css/main.css" />
    @Html.Raw(JavaScriptSnippet.FullScript)
</head>
<body>
    <header>
        <div class="wrapper clearfix header-content">
            <h2 class="azure-heading">Microsoft Azure Service Fabric</h2>
            <h1 class="party-heading">Party Clusters</h1>
            <h3 class="party-subheading">Try Service Fabric on Azure for free!</h3>

            <a class="button download" href="http://aka.ms/servicefabricSDK">Download SDK</a>
        </div>
    </header>
        @RenderBody()
    <footer>
        <div class="wrapper clearfix footer-content">
            <div class="footer-left">
                <ul>
                    <li><a href="/TermsOfUse" target="_blank">Terms of Use</a></li>
                    <li><a href="https://privacy.microsoft.com/privacystatement/ " target="_blank">Privacy &amp; Cookies</a></li>
                </ul>
            </div>
            <div class="footer-right">
                <a href="http://www.microsoft.com" target="_blank" class="copyright-logo">Microsoft</a>
                <span class="copyright">© 2016 Microsoft</span>
            </div>
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.js"></script>
    <script src="~/js/main.js" asp-append-version="true"></script>
    <script type="text/javascript">
           $(function () {
               var partyClusters = new PartyClusters(new Api());
               partyClusters.Initialize();
           });
    </script>
</body>
</html>
